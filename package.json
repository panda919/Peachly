{
    "name": "Peachly",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/Peachly/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
        "ios": "react-native run-ios",
        "iphone": "react-native run-ios --simulator='iPhone 8 Plus' ",
        "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
        "android": "react-native run-android",
        "test": "jest",
        "lint": "eslint --ext .js src",
        "lint-fix": "eslint --fix src --ext .js src",
        "format": "prettier --write src/**/*.{js,jsx}",
        "jetify": "npx jetify"
    },
    "rnpm": {
        "assets": [
            "./src/assets/fonts"
        ]
    },
    "dependencies": {
        "axios": "^0.19.0",
        "final-form": "^4.10.0",
        "final-form-arrays": "^1.0.4",
        "lodash": "^4.17.21",
        "moment": "^2.24.0",
        "native-base": "^2.12.1",
        "prop-types": "^15.7.2",
        "react": "16.8.3",
        "react-final-form": "^3.6.5",
        "react-final-form-arrays": "^1.0.6",
        "react-native": "0.59.9",
        "react-native-gesture-handler": "^1.3.0",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-loading-spinner-overlay": "^1.0.1",
        "react-native-reanimated": "^1.2.0",
        "react-native-vector-icons": "^6.6.0",
        "react-navigation": "^3.11.1",
        "react-redux": "^7.1.0",
        "redux": "^4.0.1",
        "redux-axios-middleware": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/runtime": "^7.5.5",
        "babel-eslint": "^8.2.5",
        "babel-jest": "^24.9.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-flowtype": "^2.49.3",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-prettier": "^2.6.1",
        "eslint-plugin-promise": "^3.8.0",
        "eslint-plugin-react": "^7.10.0",
        "eslint-plugin-standard": "^3.1.0",
        "jest": "^24.9.0",
        "jetifier": "^1.6.4",
        "metro-react-native-babel-preset": "^0.56.0",
        "prettier": "^1.13.7",
        "react-test-renderer": "16.8.3"
    },
    "jest": {
        "preset": "react-native"
    }
}
